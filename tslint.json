{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "member-ordering": {
        "order": "fields-first"
      },
      "max-classes-per-file": 0,
      "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
      "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}